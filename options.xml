<?xml version="1.0" encoding="UTF-8"?>
<theme defaultView="Custom">
  <formatVersion>7</formatVersion>
  <include subset="music" name="Mute" displayName="${silence}">./nosounds.xml</include>
  <include subset="music" name="On" displayName="${sysmusic}">./sounds.xml</include>
  <include subset="music" name="Off" displayName="${nomusic}" />
  <include subset="MainBGColor" name="Default" />
  <include subset="MainBGColor" name="Cyan">./colorsets/primary.xml</include>
  <include subset="MainBGColor" name="Magenta">./colorsets/secondary.xml</include>
  <include subset="MainBGColor" name="Black">./colorsets/black.xml</include>
  <include subset="MainBGColor" name="Black 2">./colorsets/black2.xml</include>
  <include subset="MainBGColor" name="Black 3">./colorsets/black3.xml</include>
  <include subset="MainBGColor" name="Blue">./colorsets/blue.xml</include>
  <include subset="MainBGColor" name="Red">./colorsets/red.xml</include>
  <include subset="MainBGColor" name="White">./colorsets/white.xml</include>
  <include subset="shader" name="None" />
  <include subset="shader" name="ScanLines">./effects/scanliness.xml</include>
  <include subset="shader" name="Crt">./effects/crtt.xml</include>
  <include subset="shader" name="Phosphore">./effects/phosphore.xml</include>
  <include subset="shader" name="Scanlines Alternative">./effects/scanliness.xml</include>
  <include subset="shader" name="Phosphore +">./effects/phosplus.xml</include>
  <include subset="shader" name="Vertical Scanlines">./effects/scanvert.xml</include>
  <include subset="shader" name="Vertical Fine">./effects/verticalfine.xml</include>
  <include subset="animbg" name="On">./effects/animatedbg2.xml</include>
  <include subset="animbg" name="Off" />
  <include subset="randomvid" name="yes" displayName="On" />
  <include subset="randomvid" name="no" displayName="Off" />
  <include subset="shader" name="Crt Full Screen" displayName="${scrtfs}">./effects/crt.xml</include>
  <include subset="shader" name="ScanLines Full Screen" displayName="${sclfs}">./effects/scanlines.xml</include>
  <include subset="shader" name="VHS Glitch (Heavy)" displayName="${svhsgl}">./effects/vhs.xml</include>
  <include subset="shader" name="CRT Glitch (Heavy)" displayName="${scrtgl}">./effects/crtvideo.xml</include>
</theme>